{
  "name": "supabase-rag-template",
  "version": "1.0.0",
  "description": "Template for Supabase RAG System - Semantic search over markdown and PDF documents",
  "main": "backend/server.js",
  "scripts": {
    "setup": "node scripts/setup.js",
    "process": "node scripts/process-documents.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/server.js",
    "dev:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "start": "node backend/server.js",
    "test": "jest",
    "test:process": "jest scripts/__tests__/process.test.js",
    "test:api": "jest backend/__tests__/api.test.js"
  },
  "keywords": [
    "rag",
    "supabase",
    "semantic-search",
    "embeddings",
    "openai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "chunkipy": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "node-cache": "^5.1.2",
    "openai": "^4.24.1",
    "p-limit": "^3.1.0",
    "tiktoken": "^1.0.10"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  }
}